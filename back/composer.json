{
    "name": "raphfelix/el-pollo",
    "description": "Back-end for the el-pollo app",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3"
    },
    "require": {
        "php": "^8.2",
        "bramus/router": "~1.6",
        "lcobucci/jwt": "^5.5",
        "mongodb/mongodb": "^2.1",
        "vlucas/phpdotenv": "^5.6",
        "lcobucci/clock": "^3.3",
        "ext-mongodb": "*"
    },
    "scripts": {
        "start": [
            "php -S 0.0.0.0:8000 -t public/"
        ]
    },
    "config": {
        "process-timeout": 0
    }
}
